@model IEnumerable<AstuteCommander.Models.BuildTabVM>

@(
Html.Kendo()
.TabStrip()
.Name("tabstrip_build_" + (string)ViewBag.Guid)
.Items(j =>
{
    foreach(var tab in @Model)
    {
        j.Add()
         .Text(tab.Name)
         .LoadContentFrom("RepoReleaseDetails", "Repository", new { id = tab.Id });
    }
})
)
